{
    "name": "hanbz/passport-client",
    "description": "Laravel wrapper around passport client OAuth2 libraries.",
    "keywords": [
        "OAuth2",
        "laravel",
        "Client"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hanbz/passport-oauth2-client/issues",
        "source": "https://github.com/hanbz/passport-oauth2-client"
    },
    "authors": [
        {
            "name": "hanbz",
            "email": "hanbzsky@yahoo.com.tw"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "league/oauth1-client": "^1.0"
    },
    "require-dev": {
        "illuminate/contracts": "^6.0|^7.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "hanbz\\PassportClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hanbz\\PassportClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "hanbz\\PassportClient\\SocialiteServiceProvider"
            ],
            "aliases": {
                "PassportClient": "hanbz\\PassportClient\\Facades\\Socialite"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
